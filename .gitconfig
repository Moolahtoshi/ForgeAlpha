[core]
	editor = \"C:\\Users\\bjara\\AppData\\Local\\Programs\\Microsoft VS Code\\bin\\code\" --wait
  autocrlf = false
  safecrlf = false
  ignorecase = false
  excludesfile = ~/.gitignore
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[branch]
  autosetuprebase = always
[diff]
  renames = copies
[color]
  ui = true
[alias]
  a = apply --index
[checkout]
  defaultRemote = origin